@model BlwTraining.ViewModels.TrainingSessionFileViewModel

@Html.HiddenFor(m => m.Id)
@Html.HiddenFor(m => m.TrainingSessionId)

<div class="form-group">
    @Html.LabelFor(m => m.FileName, new{@class="control-label"})
    @Html.TextBoxFor(m => m.FileName, new{@class="form-control"})
</div>

<div class="form-group">
    <div class="checkbox i-checks"><label> @Html.CheckBoxFor(m => m.IsDownloadable) <i></i> @Html.DisplayNameFor(m => m.IsDownloadable)</label></div>
</div>

<div class="form-group">
@Html.LabelFor(m => m.FilePath, new{@class="control-label"})
@Html.Kendo().Upload().Name("file").Multiple(false)
</div>
